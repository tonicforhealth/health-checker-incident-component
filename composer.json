{
    "name": "tonicforhealth/health-checker-incident-component",
    "description": "Health checker incident component provides classes for health checker incident app",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Gopkalo",
            "email": "drefixs@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "doctrine/orm": "~2.5",
        "swiftmailer/swiftmailer": "~5.4",
        "mtdowling/cron-expression": "~1.1",
        "danielgsims/php-collections": "~1.4",
        "puli/repository": "~1.0",
        "php-http/client-common": "~1.0",
        "php-http/plugins": "~1.0",
        "php-http/message": "~1.2",
        "php-http/discovery": "~0.6",
        "tonicforhealth/pagerduty-client": "^0.1.0"

    },
    "require-dev": {
        "phpunit/phpunit": "~4.8|~5.3",
        "php-http/mock-client": "^0.3",
        "puli/composer-plugin": "dev-classmap as 1.0-beta10",
        "php-http/guzzle6-adapter": "^1.0",
        "guzzlehttp/guzzle": "~5.3|~6.1",
        "puli/cli": "~1.0"

    },
    "suggest": {
        "guzzlehttp/guzzle": "~5.3|~6.1 for httplug client works",
        "php-http/guzzle6-adapter": "^1.0 for httplug client works"
    },
    "autoload": {
        "psr-4": { "TonicHealthCheck\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "TonicHealthCheck\\Test\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/thewilkybarkid/composer-plugin"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
